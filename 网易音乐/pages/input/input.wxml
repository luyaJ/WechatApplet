<view class="container">
  <view class="search">
      <input bindinput="inputTyping" value="{{inputVal}}" placeholder="搜索音乐、歌词、电台"/>
      <text bindtap="cancel">取消</text>		
  </view>
  
<!--当输入值后，出现搜索结果  -->
  <view class="result" wx:if="{{inputVal.length > 0}}">
    <view class="result-text">
      <view>实时搜索文本</view>
      <view>实时搜索文本</view>
    </view>
  </view>
</view>


